<ul class="nav_bar">
  <li class="nav_bar" id="active"><a href="/">Home</a></li>
  <li  class="nav_bar" style="float:right"><a class="active" href="#about">Profile</a></li>
</ul>

<%= form_tag artists_path, method: "get", html: { class: "form" } do %>
 <div class="form-group col-xs-4">
   <%= text_field_tag(:artist_name, "", placeholder: "Enter artist name", class: "form-control") %>
 </div>
 <%= submit_tag "Search", class: "btn btn-primary" %>
<% end %>

<h1 class="page-header"><%= @artist.name %></h1>
<% @artist.albums(country: 'US').each do |album| %>
 <h3><%= album.name %></h3>
 <ul>
   <% album.tracks.each do |track| %>
     <li>
       <%= track.name %>
		
     </li>
   <% end %>
 </ul>
<% end %>